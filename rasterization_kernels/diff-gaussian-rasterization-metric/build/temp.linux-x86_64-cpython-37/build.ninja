ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /root/anaconda3/envs/dense/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/root/anaconda3/envs/dense/lib/python3.7/site-packages/torch/include -I/root/anaconda3/envs/dense/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/root/anaconda3/envs/dense/lib/python3.7/site-packages/torch/include/TH -I/root/anaconda3/envs/dense/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda/include -I/root/anaconda3/envs/dense/include/python3.7m -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/root/anaconda3/envs/dense/lib/python3.7/site-packages/torch/include -I/root/anaconda3/envs/dense/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/root/anaconda3/envs/dense/lib/python3.7/site-packages/torch/include/TH -I/root/anaconda3/envs/dense/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda/include -I/root/anaconda3/envs/dense/include/python3.7m -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -I/jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/../third_party/glm/ -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_C -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_80,code=compute_80 -gencode=arch=compute_80,code=sm_80 -std=c++14
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



build /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/build/temp.linux-x86_64-cpython-37/cuda_rasterizer/backward.o: cuda_compile /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/cuda_rasterizer/backward.cu
build /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/build/temp.linux-x86_64-cpython-37/cuda_rasterizer/forward.o: cuda_compile /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/cuda_rasterizer/forward.cu
build /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/build/temp.linux-x86_64-cpython-37/cuda_rasterizer/rasterizer_impl.o: cuda_compile /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/cuda_rasterizer/rasterizer_impl.cu
build /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/build/temp.linux-x86_64-cpython-37/ext.o: compile /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/ext.cpp
build /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/build/temp.linux-x86_64-cpython-37/rasterize_points.o: cuda_compile /jfs/burning/code_bases/DenseGaussian/rasterization_kernels/diff-gaussian-rasterization-metric/rasterize_points.cu





